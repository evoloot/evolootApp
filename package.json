{
  "name": "evoloot-game",
  "version": "1.0.0",
  "description": "A game application designed for integrating Auction with Facebook",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --env.dev",
    "server": "npm-run-all compress phonegap:server",
    "phonegap:server": "phonegap serve --autoreload",
    "phonegap:android": "phonegap build android --release",
    "phonegap:ios": "phonegap build ios --release",
    "compress": "webpack --mode production --env.production",
    "build": "npm-run-all compress phonegap:android phonegap:ios",
    "lint": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KinoAR/evoloot.git"
  },
